pipeline {
    agent any
    //stages goes here
    stages {
        //this is stage 1
        stage('stage1') {
            steps {
                echo "Hello this is the stage one of the pipeline"
            }
        }
        //this is the stage 2
        stage('stage2'){
            steps{
              echo "This is the stage two of the pipeline"
            }
        }
    }
}
